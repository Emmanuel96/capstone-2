<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
</head>

<body>
    <script>
        document.addEventListener('DOMContentLoaded', () => {
            var userId = "<%= userId %>";
            axios.get(`https://business.batabest.com/setuserstatus/${userId}/visited`)
                .then(response => {
                    console.log(response);
                    localStorage.setItem('batabest_userid', userId);
                    localStorage.setItem('batabest_referrerid', response.data.referrerId);
                })

        })
    </script>
</body>

</html>